model PostImage {
  id                String         @id @default(uuid())
  
  createdAt         DateTime       @default(now())
  status            ImageStatus    @default(pending)
  imageMetaId       String?        @unique
  
  urlOriginal       String?          
  urlLarge          String?  
  urlSmall          String?

  post              Post           @relation(fields: [postId], references: [id])
  postId            String
}

