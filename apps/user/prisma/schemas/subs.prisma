model Subs {
  id                String         @id @default(uuid())

  followerId        String
  followingId       String

  createdAt         DateTime       @default(now())
  updatedAt         DateTime       @updatedAt

  follower    UserAccount @relation("UserFollowers", fields: [followerId], references: [id])
  following   UserAccount @relation("UserFollowing", fields: [followingId], references: [id])

  @@unique([followerId, followingId])
}