model UserProfile {
  id                String         @id @default(uuid())
  userName          String         @unique
  firstName         String        
  lastName          String
  birthDate         DateTime
  country           String?
  city              String?
  about             String?        @db.VarChar(255)
  createdAt         DateTime       @default(now())

  userAccount       UserAccount    @relation(fields: [userId], references: [id])
  userId            String         @unique
}