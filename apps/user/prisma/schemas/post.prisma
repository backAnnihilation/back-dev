model Post {
  id                String          @id @default(uuid())
  
  description       String?          
  imageUrl          String?         @unique
  imageId           String?         @unique

  createdAt         DateTime        @default(now())

  userAccount       UserAccount     @relation(fields: [userId], references: [id])
  userId            String

  images            PostImage[]     

  @@index([description])
}