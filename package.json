    "prisma:migrate": "node ./apps/user/prisma/tools/add-migration.js",
    "generate:app": "npx prisma generate --schema=./apps/user/prisma/schema.prisma",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:user": "nest start --project user --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/app/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config apps/second-app/test/jest-e2e.json"
  },
